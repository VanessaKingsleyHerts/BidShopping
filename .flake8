# .flake8

[flake8]

max-line-length = 100

ignore =
    E114,
    E116,
    E117,
    E222,
    E225,
    E226,
    E275,
    E741,
    W293,
    E231,
    E265,
    W391,
    E302,
    W292,
    E303,
    E501,
    F811,
    F405,
    E722,
    F841

exclude =
    venv/,
    migrations/,
    .gitlab-ci.yml,
    __pycache__/,
    static/
