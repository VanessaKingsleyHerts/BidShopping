# .flake8

[flake8]

max-line-length = 100

# Error codes to ignore
ignore =
    E114,
    E116,
    E117,
    E222,
    E225,
    E226,
    E231,
    E251,
    E265,
    E275,
    E302,
    E303,
    E401,
    E501,
    E722,
    F403,
    F405,
    F811,
    F841 

exclude =
    .venv/,
    venv/,
    migrations/,
    __pycache__/,
    static/,
    .gitlab-ci.yml
